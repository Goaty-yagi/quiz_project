y=\"datasetIdKey\"\n        :plugins=\"plugins\"\n        :css-classes=\"cssClasses\"\n        :styles=\"styles\"\n        :width=\"width\"\n        :height=\"height\"\n  />\n    </div>\n</template>\n\n<script>\nimport { Radar } from 'vue-chartjs'\nimport Chart from 'chart.js/auto';\nimport { Chart as \n    ChartJS, \n    Title, \n    Tooltip, \n    Legend, \n    PointElement,\n    RadialLinearScale,\n    LineElement,\n    Plugin } from 'chart.js'\n\nimport ChartDataLabels from 'chartjs-plugin-datalabels';\nChartJS.register(Title, Tooltip, Legend, PointElement, RadialLinearScale, LineElement, ChartDataLabels)\n\nexport default {\n  name: 'BarChart',\n  components: { Radar },\n  props: {\n    chartId: {\n      type: String,\n      default: 'bar-chart'\n    },\n    datasetIdKey: {\n      type: String,\n      default: 'label'\n    },\n    width: {\n      type: Number,\n      default: 400\n    },\n    height: {\n      type: Number,\n      default: 400\n    },\n    cssClasses: {\n      default: '',\n      type: String\n    },\n    styles: {\n      type: Object,\n      default: () => {}\n    },\n    plugins: {\n      type: Array,\n      default: () => []\n    }\n  },\n  data() {\n    return {\n      chartData: {\n        labels: [ 'ひらがな', 'カタカナ', 'ごい', 'すうじ' ],\n        datasets: [ { \n          label: \"超初級正解率\",\n          data: [10, 9, 8, 7],\n        //   fill: true,\n        //   borderDash: [9],\n        //   borderDashOffset: 0.9,\n          borderWidth:1,\n        //   clip: 'object',\n        //   order:1,\n        //   tension:1,\n        //   spanGaps: true,\n        //   borderJoinStyle: 'bevel',\n        //   pointRotation:90,\n          color: '#fff',\n          backgroundColor: 'rgba(255, 99, 132, 0.2)',\n          borderColor: 'rgb(255, 99, 132)',\n          pointBackgroundColor: 'rgb(255, 99, 132)',\n          pointBorderColor: '#fff',\n          pointHoverBackgroundColor: '#fff',\n          pointHoverBorderColor: 'red'\n          } ],\n      },\n      chartOptions: {\n          plugins: {\n            legend: {\n                labels: {\n                    // This more specific font property overrides the global property\n                    font: {\n                        size: 15\n                    }\n                }\n            }\n        },\n        responsive: false,\n        legend:{\n            labels: {\n                  fontColor: 'orange'\n                 }\n        },\n        pointLabels: { fontColor: 'red'},\n        scale:{\n          beginAtZero: true,\n          min:0,\n          max:10,\n          yAxes:[{\n              ticks:{\n                  fontColor: 'white'\n              }\n          }]\n        },\n        \n      }\n    }\n  },\n  mounted(){\n    Chart.defaults.font.size = 10\n    // Chart.defaults.font.style = 'white'\n    console.log(ChartDataLabels)\n  }\n}\n</script>\n<style scoped lang=\"scss\">\n.chart-container{\n    // background: white;\n    // font-color: white;\n}\n\n</style>"]}]}