C,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;QACxB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;QACf,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;QACb,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;QACf,CAAC;IACL,CAAC,CAAC,CAAC,CAAC,CAAC","file":"/Users/ossan/git-clone/quiz_project/vue_front/src/components/account/Chart.vue","sourceRoot":"","sourcesContent":["<template>\n    <div class=\"chart-container\">\n        <Radar\n        :chart-options=\"chartOptions\"\n        :chart-data=\"chartData\"\n        :chart-id=\"chartId\"\n        :dataset-id-key=\"datasetIdKey\"\n        :plugins=\"plugins\"\n        :css-classes=\"cssClasses\"\n        :styles=\"styles\"\n        :width=\"width\"\n        :height=\"height\"\n        />\n    </div>\n</template>\n\n<script>\nimport { Radar } from 'vue-chartjs'\nimport Chart from 'chart.js/auto';\nimport { Chart as \n    ChartJS, \n    Title, \n    Tooltip, \n    Legend, \n    PointElement,\n    RadialLinearScale,\n    LineElement,\n    Plugin } from 'chart.js'\n\nimport ChartDataLabels from 'chartjs-plugin-datalabels';\nChartJS.register(Title, Tooltip, Legend, PointElement, RadialLinearScale, LineElement, ChartDataLabels)\n\nexport default {\n    name: 'Chart',\n    components: { Radar },\n    props: {\n        chartId: {\n            type: String,\n            default: ''\n        },\n        datasetIdKey: {\n            type: String,\n            default: 'label'\n        },\n        width: {\n            type: Number,\n            default: 400\n        },\n        height: {\n            type: Number,\n            default: 400\n        },\n        cssClasses: {\n            default: '',\n            type: String\n        },\n        styles: {\n            type: Object,\n            default: () => []\n        },\n        plugins: {\n            type: Array,\n            default: () => []\n        }\n    },\n    data() {\n        Chart.defaults.color = '#fff'\n        return {\n            data:[10, 9, 8, 7],\n            chartData: {\n                labels: [ 'ひらがな', 'カタカナ', 'ボキャブラリー', 'すうじ' ],\n                datasets: [{ \n                    label: \"超初級正解率\",\n                    data: [10, 9, 8, 7],\n                    borderWidth:1,\n                    //   fill: true,\n                    //   borderDash: [9],\n                    //   borderDashOffset: 0.9,\n                    //   clip: 'object',\n                    //   order:1,\n                    //   tension:1,\n                    //   spanGaps: true,\n                    //   borderJoinStyle: 'bevel',\n                    //   pointRotation:90,\n                    backgroundColor: 'rgba(255, 153, 51, 0.2)',\n                    borderColor: ' #ff9933',\n                    pointBackgroundColor: 'rgb(255, 99, 132)',\n                    pointBorderColor: '#fff',\n                    pointHoverBackgroundColor: '#fff',\n                    pointHoverBorderColor: 'red'\n                }],\n            },\n            chartOptions: {\n                responsive: false,\n                plugins: {\n                    datalabels: {\n                        display: false\n                    }\n                },\n                scales:{\n                    r:{\n                        beginAtZero: true,\n                        min:0,\n                        max:10,\n                        grid: {\n                            color: \"gray\",\n                        },\n                        angleLines: {\n                            color: 'white',\n                        },\n                        ticks: {\n                            maxTicksLimit: 3,\n                            display: false\n                        },\n                \n                    }\n                },        \n            }\n        }\n    },\n    mounted(){\n        Chart.defaults.font.size = 16\n\n    }\n}\n</script>\n<style scoped lang=\"scss\">\n\n</style>"]}]}