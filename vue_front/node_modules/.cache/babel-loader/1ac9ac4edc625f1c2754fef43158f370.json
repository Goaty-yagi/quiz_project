background: linear-gradient(#5B759F,#1C254C);\n        width:100%;\n        height:100%;\n        }.conf-wrapper{\n            // border: solid  rgba(243, 91, 36, 0.808);\n            border-radius: 1rem;\n            overflow:hidden;\n            margin-left: auto;\n            margin-right: auto;\n            // top: 50%;\n            // left: 50%;\n            // transform: translate(-50%, -50%);\n            margin-top:35vh;\n            }\n    .button{\n    border-radius: 100vh;\n    border: 2px solid $base-color;\n    margin:1rem;\n    padding:0.4rem;\n    background: transparent;\n    color: $base-color;\n    font-weight:bold;\n    font-size: 1rem;\n    transition:0.5s;\n    &.d:hover{\n        color:green;\n        }\n    &.button-hover{\n        background: $base-color;\n        color:white;\n    }\n    }\n    .conf-body{\n        padding:2rem;\n    }\n</style>"],"sourceRoot":""}]}