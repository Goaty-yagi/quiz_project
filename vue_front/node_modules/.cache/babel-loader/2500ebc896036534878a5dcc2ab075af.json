      </div>\n            <input class='form-text' type=\"text\" v-model='discription' :placeholder=\"$store.state.description\">\n        </form>\n        <div class=\"image\">\n            <i class=\"fas fa-camera\"></i>\n            <p>写真を添付</p>\n        </div>\n        <div class=\"button-group\">\n            <p>キャンセル</p>\n            <button @click='publish'>投稿する</button>\n        </div>\n    </div>\n</template>\n\n<script>\nimport axios from 'axios'\nexport default {\n    data(){\n        return{\n            title: '',\n            discription:'',\n        }\n    },\n    mounted(){\n        this.title=this.$store.state.title\n    },\n    methods:{\n        publish(){\n            console.log('start add')\n            axios({\n                method: 'post',\n                url: '/api/board/question/create',\n                data: {\n                    title: this.$store.state.title,\n                    description: this.$store.state.description,\n                    user: this.$store.state.signup.user.uid,\n                },\n                \n            })\n            // this.$router.go({path: this.$router.currentRoute.path, force: true})\n        }\n        //     axios.post(\n        //         '/api/forum/question/',\n        //         {title: this.$store.state.title, \n        //         description: this.$store.state.description\n        //         },\n        //     ).then(response => {\n        //         console.log(response)\n        //     })\n        // },\n        // addQuestion(){\n        //     console.log('start add')\n        //     axios({\n        //         method: 'post',\n        //         url: '/api/forum/question',\n        //         data: {\n        //             title: this.$store.state.title,\n        //             description: this.$store.state.description\n        //         },\n        //     })\n        // .then((response) =>{\n            \n        // }\n\n        // )\n    }\n}\n</script>\n\n<style scoped lang='scss'>\n@import \"style/_variables.scss\";\n    .create-question-wrapper{\n        top:0;\n        position: fixed;\n        background: linear-gradient(#5B759F,#1C254C);\n        width:100vw;\n        height:100vh;\n        flex-direction: column;\n        display: flex;\n        // justify-content: center;\n        align-items: center;       \n    }\n    .title{\n        color:white;\n        margin-top: 5rem;\n        border-bottom: solid rgb(5, 5, 105);\n\n    }\n    .question-description{\n        color: white;\n    }\n    .form{\n        width: inherit;\n        // height: inherit;\n    }\n    .form-text{\n        width: 80%;\n        height:10rem;\n        border: 0.1rem solid $base-color;\n        border-radius: 1vh;\n        // text-align:left;\n    }\n    .image{\n        display:flex;\n        align-items: left;  \n        margin:1rem;\n    }\n    .button-group{\n        display:flex;\n        align-items: left;  \n        margin:1rem;\n    }\n</style>"],"sourceRoot":""}]}