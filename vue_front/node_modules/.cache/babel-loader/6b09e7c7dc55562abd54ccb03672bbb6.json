{"ast":null,"code":"import { router } from \"/src/main.js\";\nexport default {\n  data: function data() {\n    return {\n      scripts: {\n        mainTitle: '404　サーバーエラー',\n        sub: '404 Server Error',\n        sub2: 'sorry! somethign wired happened',\n        report: 'エラー報告をする',\n        return: 'Return to home'\n      }\n    };\n  },\n  mounted: function mounted() {\n    console.log('mounted', this.$store.getters.logger);\n    this.checkBeingException();\n    setTimeout(this.reload, 3000);\n  },\n  methods: {\n    closeConf: function closeConf() {\n      this.$store.commit('reset');\n    },\n    checkBeingException: function checkBeingException() {\n      this.$store.commit('checkBeingException');\n      this.$store.commit('reloadForExceptionTrue');\n    },\n    reload: function reload() {\n      console.log('reload_enter', this.$store.state.signup.beingException);\n\n      if (this.$store.state.signup.reloadForException) {\n        console.log('reload_desu');\n        this.$store.commit('reloadForExceptionFalse'); // this.goHome()\n\n        console.log('start-reload');\n        window.location.reload();\n      }\n    },\n    createLog: function createLog() {\n      console.log('in not 404 got log');\n      this.$store.dispatch('createLog', this.$store.getters.logger);\n    },\n    goHome: function goHome() {\n      router.push('/');\n    }\n  }\n};","map":{"version":3,"mappings":"AAmBA,SAAQA,MAAR,QAAqB,cAArB;AAEA,eAAe;EACXC,IADW,kBACL;IACF,OAAM;MACFC,OAAO,EAAE;QACTC,SAAS,EAAC,aADD;QAETC,GAAG,EAAC,kBAFK;QAGTC,IAAI,EAAC,iCAHI;QAITC,MAAM,EAAC,UAJE;QAKTC,MAAM,EAAC;MALE;IADP,CAAN;EAUH,CAZU;EAaXC,OAbW,qBAaF;IACLC,OAAO,CAACC,GAAR,CAAY,SAAZ,EAAsB,KAAKC,MAAL,CAAYC,OAAZ,CAAoBC,MAA1C;IACA,KAAKC,mBAAL;IACAC,UAAU,CAAC,KAAKC,MAAN,EAAc,IAAd,CAAV;EACH,CAjBU;EAkBXC,OAAO,EAAC;IACJC,SADI,uBACO;MACP,KAAKP,MAAL,CAAYQ,MAAZ,CAAmB,OAAnB;IACH,CAHG;IAIJL,mBAJI,iCAIiB;MACjB,KAAKH,MAAL,CAAYQ,MAAZ,CAAmB,qBAAnB;MACA,KAAKR,MAAL,CAAYQ,MAAZ,CAAmB,wBAAnB;IACH,CAPG;IAQJH,MARI,oBAQI;MACJP,OAA