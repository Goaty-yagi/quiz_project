ext(e)||typeof nextUrl=='string' && next()\" :class=\"!nextUrl&&DetailTagIndex==loggers.results.length-1 ? 'space-right':'fas fa-angle-double-right'\"></i>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nexport default {\n    props:[\n        'loggers',\n        'currentTagIndex',\n        'nextUrl',\n        'noMoreUrl'\n    ],\n    data(){\n        return{\n            DetailTagIndex: this.currentTagIndex,\n            logIdList: []\n        }\n    },\n    mounted(){\n        this.ckeckedTrue(0)\n        this.$store.commit('showModalTrue')\n        console.log('mounted at detail',this.nextUrl)\n    },\n    beforeUnmount(){\n        this.$store.commit('showModalFalse')\n    },\n    computed:{\n        myQuiz(){\n            return this.$store.state.signup.djangoUser.my_quiz[0]\n        },\n    },\n    methods:{\n        close(){\n            this.$emit('logDetailFalse')\n        },\n        async next(e) {\n            console.log('clicked',e)\n            if(this.DetailTagIndex!=this.loggers.results.length-1){\n                this.DetailTagIndex += 1\n            } else if (this.nextUrl&&this.DetailTagIndex==this.loggers.results.length-1){\n                this.getNextLogger()\n                this.DetailTagIndex += 1\n            }\n        },\n        ckeckedTrue(id) {\n            if(!this.loggers.results[id].checked) {\n                this.loggers.results[id].checked = true\n                this.logIdList\n            }\n            console.log(this.loggers.results)\n        },\n        back() {\n            this.DetailTagIndex -= 1\n        },\n        getNextLogger() {\n            this.$emit('getNextLogger')\n        },\n        noShow(index) {\n            if(index==0||index==5) {\n                console.log('true',index)\n                return true\n            } else {\n                return false\n            }\n        }\n    },\n}\n</script>\n\n<style scoped lang='scss'