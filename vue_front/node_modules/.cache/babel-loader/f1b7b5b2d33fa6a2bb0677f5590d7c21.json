{"ast":null,"code":"export default {\n  props: ['loggers', 'currentTagIndex', 'nextUrl'],\n  data: function data() {\n    return {\n      DetailTagIndex: this.currentTagIndex\n    };\n  },\n  mounted: function mounted() {\n    this.$store.commit('showModalTrue');\n    console.log('mounted at detail', this.questionDetailInfo);\n  },\n  beforeUnmount: function beforeUnmount() {\n    this.$store.commit('showModalFalse');\n  },\n  computed: {\n    myQuiz: function myQuiz() {\n      return this.$store.state.signup.djangoUser.my_quiz[0];\n    }\n  },\n  methods: {\n    close: function close() {\n      this.$emit('logDetailFalse');\n    },\n    next: function next() {\n      this.DetailTagIndex += 1;\n    },\n    back: function back() {\n      this.DetailTagIndex -= 1;\n    },\n    getNextLogger: function getNextLogger() {\n      this.$emit('getNextLogger');\n    },\n    noShow: function noShow(index) {\n      if (index == 0 || index == 5) {\n        console.log('true', index);\n        return true;\n      } else {\n        return false;\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AA8BA,eAAe;EACXA,KAAK,EAAC,CACF,SADE,EAEF,iBAFE,EAGF,SAHE,CADK;EAMXC,IANW,kBAML;IACF,OAAM;MACFC,cAAc,EAAE,KAAKC;IADnB,CAAN;EAGH,CAVU;EAWXC,OAXW,qBAWF;IACL,KAAKC,MAAL,CAAYC,MAAZ,CAAmB,eAAnB;IACAC,OAAO,CAACC,GAAR,CAAY,mBAAZ,EAAgC,KAAKC,kBAArC;EACH,CAdU;EAeXC,aAfW,2BAeI;IACX,KAAKL,MAAL,CAAYC,MAAZ,CAAmB,gBAAnB;EACH,CAjBU;EAkBXK,QAAQ,EAAC;IACLC,MADK,oBACG;MACJ,OAAO,KAAKP,MAAL,CAAYQ,KAAZ,CAAkBC,MAAlB,CAAyBC,UAAzB,CAAoCC,OAApC,CAA4C,CAA5C,CAAP;IACH;EAHI,CAlBE;EAuBXC,OAAO,EAAC;IACJC,KADI,mBACG;MACH,KAAKC,KAAL,CAAW,gBAAX;IACH,CAHG;IAIJC,IAJI,kBAIG;MACH,KAAKlB,cAAL,IAAuB,CAAvB;IACH,CANG;IAOJmB,IAPI,kBAOG;MACH,KAAKnB,cAAL,IAAuB,CAAvB;IACH,CATG;IAUJoB,aAVI,2BAUY;MACZ,KAAKH,KAAL,CAAW,eAAX;IACH,CAZG;IAaJI,MAbI,kBAaGC,KAbH,EAaU;MACV,IAAGA,KAAK,IAAE,CAAP,IAAUA,KAAK,IAAE,CAApB,EAAuB;QACnBjB,OAAO,CAACC,GAAR,CAAY,MAAZ,EAAmBgB,KAAnB;QACA,OAAO,IAAP;MACJ,CAHA,MAGO;QACH,OAAO,KAAP;MACJ;IACJ;EApBI;AAvBG,CAAf","names":["props","data","DetailTagIndex","currentTagIndex","mounted","$store","commit","console","log","questionDetailInfo","beforeUnmount","computed","myQuiz","state","signup","djangoUser","my_quiz","methods","close","$emit","next","back","getNextLogger","noShow","index"],"sourceRoot":"","sources":["/Users/ossan/git-clone/quiz_project/vue_front/src/components/dashboard/LogDetail.vue"],"sourcesContent":["<template>\n    <div class=\"l-wrapper\">\n        <div class=\"main-wrapper\">\n            <div class='main-quizdetail-wrapper'>\n                <div class='l-container'>\n                    <div class=\"close-container\">\n                        <div @click=\"close()\" class=\"close\">\n                            <i class=\"fas fa-times\"></i>\n                        </div>\n                    </div>\n                    <div class=\"log-index\">{{ DetailTagIndex+1 }}</div>\n                    <div class=\"main-detail-container\" :class=\"{'noShow':noShow(logindex)}\" v-for=\"(value, key ,logindex) in loggers.results[DetailTagIndex]\"\n                        v-bind:key=\"logindex\">\n                        <div v-if=\"logindex!=0&&logindex!=5\" class=\"question-field\">\n                            <p class=\"detail-text\">{{ key }}</p>\n                            <p class=\"detail-val\">{{ value }}</p>\n                        </div>\n                    </div>\n                    <div class=\"angle-container\">\n                        <!-- @click=\"e => result==false && onClick(answerindex,answer,question)\" -->\n                        <i @click=\"e => DetailTagIndex!=0&& back()\" :class=\"DetailTagIndex==0 ? 'space-left':'fas fa-angle-double-left'\"></i>\n                        <i @click=\"e => !nextUrl&&DetailTagIndex==loggers.results.length-1 && next()\" :class=\"!nextUrl&&DetailTagIndex==loggers.results.length-1 ? 'space-right':'fas fa-angle-double-right'\"></i>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nexport default {\n    props:[\n        'loggers',\n        'currentTagIndex',\n        'nextUrl'\n    ],\n    data(){\n        return{\n            DetailTagIndex: this.currentTagIndex,\n        }\n    },\n    mounted(){\n        this.$store.commit('showModalTrue')\n        console.log('mounted at detail',this.questionDetailInfo)\n    },\n    beforeUnmount(){\n        this.$store.commit('showModalFalse')\n    },\n    computed:{\n        myQuiz(){\n            return this.$store.state.signup.djangoUser.my_quiz[0]\n        },\n    },\n    methods:{\n        close(){\n            this.$emit('logDetailFalse')\n        },\n        next() {\n            this.DetailTagIndex += 1\n        },\n        back() {\n            this.DetailTagIndex -= 1\n        },\n        getNextLogger() {\n            this.$emit('getNextLogger')\n        },\n        noShow(index) {\n            if(index==0||index==5) {\n                console.log('true',index)\n                return true\n            } else {\n                return false\n            }\n        }\n    },\n}\n</script>\n\n<style scoped lang='scss'>\n@import \"style/_variables.scss\";\n.main-quizdetail-wrapper{\n    display: flex;\n    justify-content: center;\n    position: relative;\n    width: 100%;\n    .l-container{\n        position: relative;\n        width: 90%;\n        .log-index{\n            position: absolute;\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            border: solid $base-color;\n            border-radius: 50vh;\n            width: 1.5rem;\n            height: 1.5rem;\n            margin-left: 0.6rem;\n            margin-top: 0.3rem;\n            font-weight: bold;\n            background: $dark-blue;\n            color: $lite-gray;\n        }\n        .main-detail-container{\n            display: flex;\n            justify-content: center;\n            align-items: center;\n            flex-direction: column;\n            margin-top: 1rem;\n            margin-bottom: 1rem;\n            .question-field{\n                display: flex;\n                justify-content: center;\n                align-items: center;\n                flex-direction: column;\n                border-bottom: 0.2rem solid $lite-gray;\n                width: 90%;\n                .detail-text{\n                    display: flex;\n                    font-weight: bold;\n                    font-size: 1.2rem;\n                    color: rgb(167, 167, 167);\n                }\n                .detail-val{\n                    display: flex;\n                    font-weight: bold;\n                    margin-left: 0.5rem;\n                    font-size: 0.9rem;\n                }\n            }\n        }\n        .noShow{\n            display: none;\n        }\n        .angle-container{\n            display: flex;\n            justify-content: center;\n            width: 100%;\n            margin-bottom: 1rem;\n            .fa-angle-double-left{\n                margin-right: 2rem;\n                font-size: 1.2rem;\n                color: gray;\n            }\n            .space-left{\n                margin-right: 2rem;\n                width: 1.2rem;\n            }\n            .fa-angle-double-right{\n                margin-left: 2rem;\n                font-size: 1.2rem;\n                color: gray;\n            }\n            .space-right{\n                margin-left: 2rem;\n                width: 1.2rem;\n            }\n        }\n    }\n}\n\n</style>"]},"metadata":{},"sourceType":"module"}